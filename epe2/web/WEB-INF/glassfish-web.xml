<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="error.jsp">
    <class-loader delegate="true"/>
    <jsp-config>
        <property name="keepgenerated" value="false">
            <description>Do not keep a copy of the generated servlet class' java code for production.</description>
        </property>
    </jsp-config>
    <!-- Example context parameter -->
    <context-param>
        <param-name>exampleParam</param-name>
        <param-value>exampleValue</param-value>
    </context-param>
</glassfish-web-app>
